<Window x:Class="管道能耗.AboutUs"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:管道能耗"
        mc:Ignorable="d"
        Title="AboutUs" Height="300" Width="400" WindowStyle="ToolWindow">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
        </Grid.RowDefinitions>
        <Image Grid.Row="0" Source="Images/background.png"></Image>
        <Grid Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="57,13,0,0">
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"></RowDefinition>
                <RowDefinition Height="1*"></RowDefinition>
                <RowDefinition Height="1*"></RowDefinition>
            </Grid.RowDefinitions>
            <Label Grid.Row="0">项目名称：管道能耗项目</Label>
            <Label Grid.Row="1">开发团队：指尖改变世界</Label>

        </Grid>
    </Grid>
</Window>
